package IOExample
version v2r8

branches doc options cmt src
include_path none

use GaudiKernel      v*
use GaudiAlg         v*
use TrackEvent       v* Event

# Uses only needed to set up run time environment
private
  use GaudiSvc            v*              -no_auto_imports
  use GaudiPoolDb         v*              -no_auto_imports
  use GaudiConf           v*              -no_auto_imports
  use EventSys            v* Event        -no_auto_imports
  macro CASTOR_use "" QMTest "CASTOR v* LCG_Interfaces"
  use $(CASTOR_use)
  apply_pattern QMTest
end_private


library IOExample     *.cpp
apply_pattern component_library library=IOExample
