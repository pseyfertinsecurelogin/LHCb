<?xml version="1.0" ?><!DOCTYPE extension  PUBLIC '-//QM/2.3/Extension//EN'  'http://www.codesourcery.com/qm/dtds/2.3/-//qm/2.3/extension//en.dtd'>
<extension class="GaudiTest.GaudiExeTest" kind="test">
  <argument name="program"><text>gaudirun.py</text></argument>
  <argument name="args"><set><text>../options/CopyPOOLtoROOTsim.py</text></set></argument>
  <argument name="reference"><text>../refs/POOLSim.ref</text></argument>
  <argument name="validator"><text>
from GaudiConf.QMTest.LHCbExclusions import preprocessor as LHCbPreprocessor
preprocessor = LHCbPreprocessor + \
  LineSkipper(["StoreExplorerAlg     INFO"]) + \
  LineSkipper(["TESFingerPrint"]) + \
  LineSkipper(["EventPersistenc...   INFO  'CnvServices':"]) + \
  LineSkipper(["InputCopyStream      INFO"]) + \
  LineSkipper(["EventSelector        INFO Stream:EventSelector.DataStreamTool_1 Def:DATAFILE"]) + \
  LineSkipper(["FSRInputCopyStream   INFO "]) + \
  LineSkipper(["FileRecordPersi...   INFO  'CnvServices':[ 'Gaudi::RootCnvSvc/FileRecordCnvSvc' ]"]) + \
  LineSkipper(["FileRecordPersi...   INFO Added successfully Conversion service:FileRecordCnvSvc"]) + \
  LineSkipper(["FileRecordDataSvc    INFO Failed to register record for:00000000-0000-0000-0000-000000000000 [Invalid Address]"]) + \
  LineSkipper(["PoolRootEvtCnvSvc    INFO"]) + \
  LineSkipper(["EventPersistenc...   INFO Added successfully Conversion service:"])
validateWithReference(preproc = preprocessor)
</text></argument>
  <argument name="prerequisites"><set>
    <tuple><text>ioexample.copypoolsimtopool</text><enumeral>PASS</enumeral></tuple>
  </set></argument>
</extension>
