<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id: tabproperty.dtd,v 1.1 2001-12-17 15:46:14 andreav Exp $ -->


<!-- Tabulated Property NameReference   (V.B., March'2k+1 3) -->
<!ELEMENT tabprops EMPTY>
<!ATTLIST tabprops
      address CDATA #REQUIRED 
>

<!-- Tabulated Property Entry           (V.B., March'2k+1 3) -->
<!ELEMENT entry EMPTY>
<!ATTLIST entry 
    x     CDATA #REQUIRED 
    y     CDATA #REQUIRED 
    text  CDATA #IMPLIED
>

<!-- Tabulated Property Table           (V.B., March'2k+1 3) -->
<!ELEMENT tabproperty  (#PCDATA|entry)* >
<!ATTLIST tabproperty
    xml:space (default|preserve) #FIXED "preserve"
    name    ID     #REQUIRED
    type    CDATA  #REQUIRED 
    xaxis   CDATA  "xAxis"
    yaxis   CDATA  "yAxis" 
    xunit   CDATA  "1.0"
    yunit   CDATA  "1.0"
>

<!-- Tabulated Property Table Reference (V.B. March'2k+1 3) -->
<!ELEMENT tabpropertyref EMPTY>
<!ATTLIST tabpropertyref
  href    CDATA    #REQUIRED
>

<!-- End of the DTD file -->
