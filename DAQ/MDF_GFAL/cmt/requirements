# ==================== libraries ==================================
library MDF_GFAL *.cpp -import=MDF -import=gfal
apply_pattern ld_library_path
apply_pattern linker_library library=MDF_GFAL

apply_pattern disable_package_on platform=Darwin
apply_pattern disable_package_on platform=WIN32
