<?xml version="1.0" ?><!DOCTYPE extension  PUBLIC '-//QM/2.3/Extension//EN'  'http://www.codesourcery.com/qm/dtds/2.3/-//qm/2.3/extension/
/en.dtd'>
<extension class="GaudiTest.GaudiExeTest" kind="test">
<argument name="program"><text>gaudirun.py</text></argument>
<argument name="args"><set>
  <text>-v</text>
  <text>${DETDESCCHECKSROOT}/options/LoadDDDB.py</text>
</set></argument>
<argument name="options"><text>
from Gaudi.Configuration import *
from Configurables import GitEntityResolver, DDDBConf, CondDB
from Configurables import DetCondTest__TestConditionAlg as TestAlg

import os

GitEntityResolver(PathToRepository=os.environ['GIT_TEST_REPOSITORY'],
                  OutputLevel=VERBOSE)

DDDBConf(DataType = "2016")
CondDB(EnableRunStampCheck = False,
       Tags={'DDDB': 'v0'})

ApplicationMgr().TopAlg.append(TestAlg(Conditions=['/dd/Changing']))
</text></argument>
<argument name="validator"><text>
from subprocess import check_output
import os
import re

tag = 'v0'
repository = os.environ['GIT_TEST_REPOSITORY']
commit_id = check_output(['git', 'rev-parse', tag], cwd=repository)
parameter = 0

countErrorLines()

findReferenceBlock('''
ToolSvc.GitEnti...  DEBUG opening Git repository '{repository}'
ToolSvc.GitEnti...  DEBUG Initializing...
ToolSvc.GitEnti...  DEBUG commit '{tag}' corresponds to {commit_id}
ToolSvc.GitEnti...  DEBUG Successfully initialized.
'''.format(repository=repository, commit_id=commit_id, tag=tag),
id='GitEntityResolver.initialization')

findReferenceBlock('''
LoadDDDB             INFO Database {repository} tag {tag}
'''.format(repository=repository, commit_id=commit_id, tag=tag),
id='reported_tag')

# grep
stripped_output = '\n'.join(l for l in stdout.splitlines()
                            if re.match(r'^---|^Validity|^\(int\) parameter', l))

findReferenceBlock('''
--- /dd/Changing
Validity: 0.0 -> 9223372036.854775807
(int) parameter = {parameter}
'''.format(parameter=parameter),
stdout=stripped_output,
id='condition_value')

</text></argument>
<argument name="prerequisites"><set>
  <tuple><text>gitentityresolver.prepare</text><enumeral>PASS</enumeral></tuple>
</set></argument>
<argument name="use_temp_dir"><enumeral>true</enumeral></argument>
</extension>
