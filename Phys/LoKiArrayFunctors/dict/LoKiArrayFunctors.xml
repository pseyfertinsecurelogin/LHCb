  <class pattern   = "LoKi::Dicts::CutCalls<*Range_*LHCb::Particle*>"   />
  
  <function pattern = "LoKi::Dicts::monitor*"            />
  
  <class pattern   = "LoKi::IHybrid*"    />
  <class pattern   = "LoKi::Hybrid::*"   />


