  <class pattern   = "std::unary_function<*Range_*LHCb::Particle*>"          />
  <class pattern   = "std::binary_function<*Range_*LHCb::Particle*>"          />
  
  <class pattern = "LoKi::Range_<std::vector<LHCb::Particle*> >" />
  
