package             CaloUtils
version             v6r4

# ================= structure =================================================
branches            cmt doc src CaloUtils

# ================= own includes ==============================================
include_dirs        $(CALOUTILSROOT)

# ================= dependencies ==============================================
use CaloInterfaces   v*      Calo 
use TrackInterfaces  v*      Tr
use CaloDet          v*      Det 
use RecEvent         v*      Event
use TrackEvent       v*      Event 
use LHCbMath         v*      Kernel

# ================= constituents: linker library ==============================
library             CaloUtils    *.cpp

# ================= patterns ==================================================
apply_pattern       linker_library library=CaloUtils

