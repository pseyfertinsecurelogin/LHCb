
pythonexe     = $(Python_home)\python.exe
parsetool     = $(GAUDIOBJDESCROOT)\scripts\godII.py
dest          = ..\$(tag)\dict\$(package)\ 
dtdfile       = ..\$(xmlsrc)\gdd.dtd 

${CONSTITUENT}_output = $(dest)

${CONSTITUENT} :: $(dtdfile) $(dest)$(package).lcgdict
	@echo "-----> ${CONSTITUENT} ok"

$(dtdfile) : $(GAUDIOBJDESCROOT)\xml_files\gdd.dtd
	@echo "Copying global DTD to current package"
	@copy $(GAUDIOBJDESCROOT)\xml_files\gdd.dtd $(dtdfile)
