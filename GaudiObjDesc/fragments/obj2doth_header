$(dtdfile) : $(GAUDIOBJDESCROOT)/xml_files/gdd.dtd
	@echo "Copying global DTD to current package"
	@cp $(GAUDIOBJDESCROOT)/xml_files/gdd.dtd $(dtdfile)
