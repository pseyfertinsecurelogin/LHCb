$(dest)${NAME}.obj2doth : ${FULLNAME} 
	@echo Producing Header files from ${FULLNAME}
	@if !(test -d $(dest)); then mkdir $(dest); fi 
	export LD_LIBRARY_PATH=$(Python_home)/lib; cd $(dest); $(pythonexe) $(parsetool) $(GODflags) $(${CONSTITUENT}_GODflags) -g src -r $(GAUDIOBJDESCROOT) ${FULLNAME}
	@echo /dev/null > $@
