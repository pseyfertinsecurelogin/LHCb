<extension class="GaudiTest.GaudiExeTest" kind="test">
<argument name="program"><text>gaudirun.py</text></argument>
<argument name="args"><set>
  <text>-v</text>
  <text>../../options/scheduler_testCF.py</text>
</set></argument>
<argument name="validator"><text>
expected_strings = [ "NONLAZY_AND: moore     0|0",
                     " LAZY_AND: line2       0|0",
                     "  A3                   0|0",
                     "  A4                   1|1",
                     " NONLAZY_OR: decision  0|1",
                     "  LAZY_OR: line1       0|1",
                     "   A1                  0|1",
                     "   A2                  1|1",
                     "  A5                   0|1",
                     "  NOT: notA1           0|0",
                     "   A1                  0|1",
                     "A1                  1",
                     "A2                  1",
                     "A5                  1",
                     "A3                  1",
                     "A4                  0" ]

nodeorder = "ordered nodes: [A3, A4, A5, A1, A2]"


for expected_string in expected_strings:
    occurrences = 0
    index = 0
    while True:
        index = stdout.find(expected_string, index)
        if index == -1:
            break
        index += 1
        occurrences += 1
    if occurrences &lt; 4:
        causes.append('control flow gone wrong for {}'.format(expected_string))

if stdout.find(nodeorder) == -1:
    causes.append('node order gone wrong')
</text></argument>
</extension>

