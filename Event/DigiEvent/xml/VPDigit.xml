           author = "Marcin Kucharczyk"
           desc = "Digitized pixel hit in VP">
    <location name = "Default" place = "Raw/VP/Digits"/>
    <import name = "Kernel/VPChannelID"/>
    <template name = "KeyedObjectDict" t1 = "LHCb::VPChannelID"/>
