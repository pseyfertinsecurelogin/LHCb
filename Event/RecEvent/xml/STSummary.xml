<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE gdd SYSTEM 'gdd.dtd'>
<gdd>
  <package name='RecEvent'>

  <class author="Matthew Needham" desc="Silicon tracker summary class" id="9005" name="STSummary" >
      <base name="DataObject"/>
      &DataObject;
      <location name="TTSummary"   place="Rec/TT/Summary"   />
      <location name="ITSummary"   place="Rec/IT/Summary"   />
       <constructor argList="unsigned int nClusters, unsigned int pcn, bool pcnSynch , std::vector&lt; unsigned int &gt; banksWithError, std::vector&lt; unsigned int &gt; missing " 
                    initList="m_nClusters(nClusters), m_pcn(pcn), m_pcnSynch(pcnSynch), m_banksWithError(banksWithError), m_missingBanks(missing)" desc="special constructor">
       <code></code>
      </constructor>
      <attribute name="nClusters" type="unsigned int" setMeth = "FALSE" desc="number of clusters"/>
      <attribute name="pcn" type="unsigned int" setMeth = "FALSE" desc="pcn"/>
      <attribute name="pcnSynch" type="bool" setMeth = "FALSE" desc="pcn agree"/>
      <attribute name="banksWithError" type="std::vector &lt; unsigned int &gt;" setMeth = "FALSE" desc="banks with error"/>
      <attribute name="missingBanks" type="std::vector &lt; unsigned int &gt;" setMeth = "FALSE" desc="banks missed"/>
      <method name = "hasError" type = "bool" const = "TRUE" desc = "data is good or not">
         <code>
            bool good = m_pcnSynch &amp; m_banksWithError.empty(); 
            return !good; 
         </code>
      </method>
  </class>
 </package>
</gdd>
