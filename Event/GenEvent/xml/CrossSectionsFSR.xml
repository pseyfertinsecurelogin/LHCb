        <enumval name="FractionDGen"         value="85"   desc="Fraction of generated D"/>
	<enumval name="FractionDstarGen"     value="86"   desc="Fraction of generated D*"/>
	<enumval name="FractionD2starGen"    value="87"   desc="Fraction of generated D**"/>
