package           EventAssoc
version           v3r3

# =============== structure ===================================================
branches          cmt doc dict 

# =============== dependencies ================================================
use               L0Event     v* Event
use               PhysEvent   v* Event
use               MCEvent     v* Event
use               Relations   v* Kernel

# ========= temporary fix for the LCG_48 release cycle =======================
use               HltEvent    v* Event
use               DAQEvent    v* Event
use               GenEvent    v* Event
use               LinkerEvent v* Event
use               PackedEvent v* Event
use               VeloEvent   v* Event
# ========= end of temporary fix for LCG_48 ==================================
        
#private               
use               GaudiPython v* -no_auto_imports 
        
# =============== LCG/Reflex dictionaries =====================================
apply_pattern     reflex_dictionary                              \ 
                  dictionary=EventAssoc                          \
                  headerfiles=$(EVENTASSOCROOT)/dict/selection.h \
                  selectionfile=../dict/selection.xml            \
                  options="-U__MINGW32__"

# =============== standard patterns ===========================================
apply_pattern     component_library library=EventAssocDict

