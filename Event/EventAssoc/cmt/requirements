package           EventAssoc
version           v4r6p1

# =============== structure ===================================================
branches          cmt doc dict 

# =============== dependencies ================================================
use               L0Event     v* Event
use               GenEvent    v* Event
use               MCEvent     v* Event
use               TrackEvent  v* Event
use               PhysEvent   v* Event
use               Relations   v* Kernel

use               GaudiPython v* -no_auto_imports 
        
# =============== LCG/Reflex dictionaries =====================================
apply_pattern     reflex_dictionary                              \ 
                  dictionary=EventAssoc                          \
                  headerfiles=$(EVENTASSOCROOT)/dict/selection.h \
                  selectionfile=../dict/selection.xml            \
                  options="-U__MINGW32__"

# Hack for Gaudi v21r1 to link dictionary on windows
private
set EXPORT_ALL_SYMBOLS "yes" GAUDI_V21 "" GAUDI_V22 "" G21_HIDE_SYMBOLS ""
